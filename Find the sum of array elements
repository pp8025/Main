
#include <stdio.h>

void main() {
    int a[10], sum = 0, n, i;

    printf("Enter limit: ");
    scanf("%d", &n);

    printf("Enter elements: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        sum = sum + a[i];
    }

    printf("Sum of array elements: %d", sum);
    getch();
}
